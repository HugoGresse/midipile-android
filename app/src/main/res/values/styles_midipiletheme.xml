    <style name="ActionBarSolidMidipileTheme" parent="@android:style/Widget.Holo.Light.ActionBar.Solid">
        <item name="android:background">@drawable/ab_solid_midipile</item>
        <item name="android:backgroundStacked">@drawable/ab_stacked_solid_midipile</item>
        <item name="android:backgroundSplit">@drawable/ab_bottom_solid_midipile</item>
        <item name="android:progressBarStyle">@style/ProgressBar.Midipile</item>
    </style>

    <style name="ProgressBar.Midipile" parent="@android:style/Widget.Holo.Light.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/progress_horizontal_midipile</item>
    </style>

  <style name="AutoCompleteTextViewMidipileTheme" parent="android:Widget.Holo.Light.AutoCompleteTextView">
      <item name="android:dropDownSelector">@drawable/midipiletheme_list_selector_holo_light</item>
	    <item name="android:background">@drawable/midipiletheme_edit_text_holo_light</item>
  </style>

  <style name="ButtonMidipileTheme" parent="android:Widget.Holo.Light.Button">
	  <item name="android:background">@drawable/midipiletheme_btn_default_holo_light</item>
  </style>

  <style name="ImageButtonMidipileTheme" parent="android:Widget.Holo.Light.ImageButton">
	  <item name="android:background">@drawable/midipiletheme_btn_default_holo_light</item>
  </style>

  <style name="SpinnerMidipileTheme" parent="android:Widget.Holo.Light.Spinner">
      <item name="android:background">@drawable/midipiletheme_spinner_background_holo_light</item>
      <item name="android:dropDownSelector">@drawable/midipiletheme_list_selector_holo_light</item>
  </style>

  <style name="TabMidipileTheme">
      <item name="android:gravity">center_horizontal</item>
      <item name="android:paddingLeft">16dip</item>
      <item name="android:paddingRight">16dip</item>
      <item name="android:background">@drawable/midipiletheme_tab_indicator_holo</item>
      <item name="android:layout_width">0dip</item>
      <item name="android:layout_weight">1</item>
      <item name="android:minWidth">80dip</item>
  </style>

  <style name="TabTextMidipileTheme">
	  <item name="android:textColor">#000000</item>
      <item name="android:textSize">12sp</item>
      <item name="android:textStyle">bold</item>
      <!-- v14 <item name="android:textAllCaps">true</item> -->
      <item name="android:ellipsize">marquee</item>
      <item name="android:maxLines">2</item>
      <item name="android:maxWidth">180dip</item>
  </style>

  <style name="ProgressBarMidipileTheme" parent="android:Widget.Holo.Light.ProgressBar.Horizontal">
      <item name="android:progressDrawable">@drawable/midipiletheme_progress_horizontal_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/midipiletheme_progress_indeterminate_horizontal_holo_light</item>
  </style>

  <style name="SeekBarMidipileTheme" parent="android:Widget.Holo.Light.SeekBar">
      <item name="android:progressDrawable">@drawable/midipiletheme_scrubber_progress_horizontal_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/midipiletheme_scrubber_progress_horizontal_holo_light</item>
      <item name="android:thumb">@drawable/midipiletheme_scrubber_control_selector_holo_light</item>
  </style>

  <style name="RatingBarMidipileTheme" parent="android:Widget.Holo.Light.RatingBar">
      <item name="android:progressDrawable">@drawable/midipiletheme_ratingbar_full_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/midipiletheme_ratingbar_full_holo_light</item>
  </style>

  <style name="ToggleMidipileTheme" parent="android:Widget.Holo.Light.Button.Toggle">
      <item name="android:background">@drawable/midipiletheme_btn_toggle_holo_light</item>
